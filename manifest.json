{
	"manifest_version": 3,
	"name": "KingsNet",
	"version": "1.0",
	"description": "Crannet but with old assignment center",
	"permissions": [
		"storage",
		"declarativeNetRequest",
		"activeTab",
		"scripting"
	],
	"background": {
		"service_worker": "js/background.js"
	},
	"host_permissions": [
		"*://cranbrook.myschoolapp.com/*"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "ruleset_1",
				"enabled": true,
				"path": "rules.json"
			}
		]
	},
	"content_scripts": [
		{
			"matches": [
				"*://cranbrook.myschoolapp.com/*"
			],
			"js": [
				"js/content.js"
			]
		}
	]
}